    "keywords": [
        "angular",
        "webpack",
        "es6"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/angularclass/NG6-starter.git"
    },
    "author": "AngularClass",
    "license": "Apache-2.0",
    "babel": {
        "plugins": [
            "transform-runtime"
        ],
        "presets": [
            "es2015",
            "stage-0"
        ]
